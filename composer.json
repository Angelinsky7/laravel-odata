{
    "name": "flat3/odata",
    "description": "Flat3 OData for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Lloyd",
            "email": "chrislloyd403@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Flat3\\OData\\": "src/",
            "Flat3\\OData\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "ext-simplexml": "*",
        "ext-json": "*",
        "ext-dom": "*",
        "ext-pdo": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flat3\\OData\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "php": "^7.4|^8.0",
        "phpunit/phpunit": "^8.4|^9.0",
        "orchestra/testbench": "^6.0",
        "spatie/phpunit-snapshot-assertions": "^4.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
