{
    "name": "flat3/lodata",
    "type": "library",
    "description": "Flat3 OData for Laravel",
    "keywords": ["odata", "laravel"],
    "homepage": "https://github.com/flat3/lodata",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Lloyd",
            "email": "chrislloyd403@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Flat3\\Lodata\\": "src/",
            "Flat3\\Lodata\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "ext-simplexml": "*",
        "ext-json": "*",
        "ext-dom": "*",
        "ext-pdo": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flat3\\Lodata\\ServiceProvider"
            ],
            "aliases": {
                "Lodata": "Flat3\\Lodata\\Facades\\Lodata"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0",
        "orchestra/testbench": "^6.0",
        "spatie/phpunit-snapshot-assertions": "^4.2",
        "guzzlehttp/guzzle": "^6.5.5|^7.0.1",
        "doctrine/dbal": "^2.11"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
